    <content>&lt;p dir="ltr" style="text-align: left;"&gt;&lt;/p&gt;&lt;div&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{47d84eb8-228d-4ae3-9d37-4246f9dbedcb}{49}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:10,&amp;quot;335559731&amp;quot;:0,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}"&gt;&lt;span data-scheme-color="@3F3F3F,," data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;Provide learners with one community-building vocabulary word each week, such as:&lt;/span&gt;​&lt;/p&gt;&lt;/div&gt;&lt;ul role="list"&gt;&lt;li data-margin="1445" data-aria-posinset="1" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{47d84eb8-228d-4ae3-9d37-4246f9dbedcb}{52}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:1445,&amp;quot;335559731&amp;quot;:-232,&amp;quot;335559737&amp;quot;:0,&amp;quot;335562764&amp;quot;:2,&amp;quot;335562765&amp;quot;:1.14,&amp;quot;335562766&amp;quot;:4,&amp;quot;335562767&amp;quot;:0,&amp;quot;335562768&amp;quot;:4,&amp;quot;335562769&amp;quot;:6}" data-indent="-232"&gt;&lt;span data-scheme-color="@3F3F3F,," data-usefontface="true" data-contrast="none" xml:lang="EN-US" lang="EN-US"&gt;communication&lt;/span&gt;​&lt;/p&gt;&lt;/li&gt;&lt;li data-margin="1445" data-aria-posinset="2" role="listitem" data-aria-level="1"&gt;&lt;p xml:lang="EN-US" lang="EN-US" paraid="0" paraeid="{47d84eb8-228d-4ae3-9d37-4246f9dbedcb}{55}" data-ccp-props="{&amp;quot;335551550&amp;quot;:1,&amp;quot;335551620&amp;quot;:1,&amp;quot;335559683&amp;quot;:0,&amp;quot;335559685&amp;quot;:1445,&amp;quot;335559731&amp;quot;:-23